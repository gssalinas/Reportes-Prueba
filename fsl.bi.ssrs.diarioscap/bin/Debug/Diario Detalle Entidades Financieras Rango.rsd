<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSet Name="DataSet1">
    <Query>
      <DataSourceReference>dsCAPTACIONES</DataSourceReference>
      <CommandText>DEFINE                                                         
MEASURE 'Saldo Captacion'[Interes Bs] = SUMX('Saldo Captacion',[Tasa]*[montoBs])/100 +
                                     SUMX('Saldo BSEI',     [Tasa]*[montoBs])/100
--saldo anual de los ultimos 5 años,  entidades financieras
EVALUATE
UNION(
   ADDCOLUMNS(
   CALCULATETABLE(
		SUMMARIZECOLUMNS(Fecha[Fecha]
		                 ,Captacion[productoSinMoneda]
					     ,"Saldo USD con BSEI", [Saldo USD con BSEI]
						 ,"Cantidad de Contratos",[Cantidad de Contratos con BSEI]
						 ,"Interes Bs",'Saldo Captacion'[Interes Bs]
						 ,"Saldo Bs con BSEI",[Saldo Bs con BSEI]
			    )
			    ,fecha[esFindeMes] = true()
			    ,MONTH(Fecha[fecha])=12
		        ,Cliente[Clasificacion SIF] in {"Entidades Financieras"}
		)
		,"ORDEN",1    
		,"RANGO","ANUAL"
		)
,
--saldo mensual deL año ACTUAL,  entidades financieras
    ADDCOLUMNS(
    CALCULATETABLE(
		SUMMARIZECOLUMNS(Fecha[Fecha],
		                 Captacion[productoSinMoneda]
		                 ,"Saldo USD con BSEI", [Saldo USD con BSEI]
						 ,"Cantidad de Contratos",[Cantidad de Contratos con BSEI]
						 --tpp=divide(Interes Bs,saldo)
						 ,"Interes Bs",'Saldo Captacion'[Interes Bs]
						 ,"Saldo Bs con BSEI",[Saldo Bs con BSEI]
			    )
			    ,fecha[esFindeMes] = true()
               ,DATESBETWEEN(Fecha[fecha],date(YEAR(LASTDATE( Fecha[fecha])), 01,01),LASTDATE(Fecha[fecha]))     
               ,Cliente[Clasificacion SIF] in {"Entidades Financieras"} 
               )
		,"ORDEN",1    
		,"RANGO","MENSUAL"
		)               
,
--saldo diario ultimas 2 semanas,  entidades financieras
	ADDCOLUMNS(
    CALCULATETABLE(
		SUMMARIZECOLUMNS(Fecha[Fecha],
		                 Captacion[productoSinMoneda]
		                  ,FILTER(fecha, fecha[semid] &gt;= max(fecha[semid])-1)
		                 ,"Saldo USD con BSEI", [Saldo USD con BSEI]
						 ,"Cantidad de Contratos",[Cantidad de Contratos con BSEI]
						 --tpp=divide(Interes Bs,saldo)
						 ,"Interes Bs",'Saldo Captacion'[Interes Bs]
						 ,"Saldo Bs con BSEI",[Saldo Bs con BSEI]
			    )
				,Cliente[Clasificacion SIF] in   {"Entidades Financieras"}
				)
		,"ORDEN",1    
		,"RANGO","SEMANAL"
		) 
--saldo anual de los ultimos 5 años,  entidades financieras		
,
   ADDCOLUMNS(
   CALCULATETABLE(
		SUMMARIZECOLUMNS(Fecha[Fecha]
		                 , Cliente[Clasificacion SIF]
					     ,"Saldo USD con BSEI", [Saldo USD con BSEI]
						 ,"Cantidad de Contratos",[Cantidad de Contratos con BSEI]
						 ,"Interes Bs",'Saldo Captacion'[Interes Bs]
						 ,"Saldo Bs con BSEI",[Saldo Bs con BSEI]
			    )
			    ,fecha[esFindeMes] = true()
			    ,MONTH(Fecha[fecha])=12
		        ,Cliente[Clasificacion SIF] in  {"Entidades Financieras Medpl","Bonos Subordinados","Subordinadas Exterior"}
		)
		,"ORDEN",2    
		,"RANGO","ANUAL"
		)
,
--saldo mensual deL año ACTUAL,  entidades financieras
    ADDCOLUMNS(
    CALCULATETABLE(
		SUMMARIZECOLUMNS(Fecha[Fecha],
		                  Cliente[Clasificacion SIF]
		                 ,"Saldo USD con BSEI", [Saldo USD con BSEI]
						 ,"Cantidad de Contratos",[Cantidad de Contratos con BSEI]
						 --tpp=divide(Interes Bs,saldo)
						 ,"Interes Bs",'Saldo Captacion'[Interes Bs]
						 ,"Saldo Bs con BSEI",[Saldo Bs con BSEI]
			    )
			    ,fecha[esFindeMes] = true()
               ,DATESBETWEEN(Fecha[fecha],date(YEAR(LASTDATE( Fecha[fecha])), 01,01),LASTDATE(Fecha[fecha]))     
               ,Cliente[Clasificacion SIF] in  {"Entidades Financieras Medpl","Bonos Subordinados","Subordinadas Exterior"} 
               )
		,"ORDEN",12   
		,"RANGO","MENSUAL"
		)               
,
--saldo diario ultimas 2 semanas,  entidades financieras
	ADDCOLUMNS(
    CALCULATETABLE(
		SUMMARIZECOLUMNS(Fecha[Fecha],
		                  Cliente[Clasificacion SIF]
		                  ,FILTER(fecha, fecha[semid] &gt;= max(fecha[semid])-1)
		                 ,"Saldo USD con BSEI", [Saldo USD con BSEI]
						 ,"Cantidad de Contratos",[Cantidad de Contratos con BSEI]
						 --tpp=divide(Interes Bs,saldo)
						 ,"Interes Bs",'Saldo Captacion'[Interes Bs]
						 ,"Saldo Bs con BSEI",[Saldo Bs con BSEI]
			    )
				,Cliente[Clasificacion SIF] in   {"Entidades Financieras Medpl","Bonos Subordinados","Subordinadas Exterior"}
				)
		,"ORDEN",2    
		,"RANGO","SEMANAL"
		) 
)</CommandText>
